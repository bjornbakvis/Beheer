{
  "version": 2,
  "functions": {
    "api/explain-rule.py": {
      "maxDuration": 30,
      "memory": 1024
    },
    "api/**/*.py": {
      "maxDuration": 10,
      "memory": 1024
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
